    p_partkey = temp_ps_partkey
        and p_partkey = ps_partkey
        and s_suppkey = ps_suppkey
        and p_size = 15
        and p_type like '%BRASS'
        and s_nationkey = n_nationkey
		and r_name = 'EUROPE'
        and ps_supplycost = temp_min_ps_supplycost
order by
        s_acctbal desc,
        n_name,
        s_name,
        p_partkey
limit 100
;
