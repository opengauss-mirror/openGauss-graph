-- problem: test tinyint && smallint && int && bigint datatype
CREATE TABLE cstore_cmpr_test_each_int
(
	a1 varchar(32),
	a2 tinyint,
	a3 smallint,
	a4 int,
	a5 bigint,
	a7 bigint,
	a8 bigint,
	a9 "char"
) with ( orientation = orc, compression = no ) tablespace hdfs_ts distribute by hash(a1);
