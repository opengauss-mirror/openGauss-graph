               Hash Cond: (t2.c3 = t3.c1)
               ->  Seq Scan on tt4x t2
               ->  Hash
                     ->  Seq Scan on tt4x t3
         ->  Hash
               ->  Hash Join
                     Hash Cond: (t4.c2 = t5.c1)
                     ->  Seq Scan on tt4x t4
                     ->  Hash
                           ->  Seq Scan on tt4x t5
   ->  Hash
         ->  Seq Scan on tt4x t1
