CREATE TABLE products_col_multi
(
product_id INTEGER,
product_name VARCHAR(60),
category VARCHAR(60),
total INTEGER
)
WITH (ORIENTATION=column)
DISTRIBUTE BY HASH(product_id, category);
INSERT INTO products_col_multi SELECT * FROM products_base;
ANALYZE products_col_multi;
BEGIN;
